  FILE *subor;
    FILE *vystup;
    int cislo, aktualne, pocet;

    subor = fopen("lotoVstup.txt", "r");
    vystup = fopen("statistikaLoto.txt", "w");

    cislo = 1;
    while (cislo <= 49)
    {
        fclose(subor);
        subor = fopen("lotoVstup.txt", "r");

        pocet = 0;

        while (fscanf(subor, "%d", &aktualne) == 1)
        {
            if (aktualne == cislo)
            {
                pocet = pocet + 1;
            }
        }

        printf("Cislo %d sa vyskytuje %d-krat.\n", cislo, pocet);
        fprintf(vystup, "Cislo ");
        fprintf(vystup, "%d", cislo);
        fprintf(vystup, " sa vyskytuje ");
        fprintf(vystup, "%d", pocet);
        fprintf(vystup, "-krat.\n");

        cislo = cislo + 1;
    }

    fclose(subor);
    fclose(vystup);


    return 0;
